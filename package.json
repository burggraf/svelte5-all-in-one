{
  "name": "sv1",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build-old": "vite build && bun run postbuild",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "bun run test:unit -- --run && bun run test:e2e",
    "test:e2e": "playwright test",
    "postbuild": "bunx svelte-kit sync",
    "postbuild-old": "bunx svelte-kit sync && svelte-kit build",
    "ios": "bun run build && bunx cap sync && bunx cap update ios && bunx cap copy ios && bunx cap run ios",
    "ios-reload": "cap run ios -l --external --port=5173",
    "android": "bun run build && bunx cap sync && bunx cap update android && bunx cap copy android && bunx cap run android",
    "android-reload": "cap run android -l --external --port=5173",
	"tauri": "tauri",
	"build-tauri": "bun run build && tauri build"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@playwright/test": "^1.45.3",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tauri-apps/cli": "1.0.0",
    "@types/eslint": "^9.6.0",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^0.21.16",
    "clsx": "^2.1.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "globals": "^15.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-kit": "^1.2.0",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.4",
    "vitest": "^2.0.4"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@inlang/paraglide-sveltekit": "^0.11.1"
  }
}
